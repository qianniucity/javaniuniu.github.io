        Object oINotificationManager = mAsInterface.invoke(null,oNotificationManagerService);
        Method mGetZenMode = cINotificationManagerStub.getMethod("getZenMode");
        zenMode = (int) mGetZenMode.invoke(oINotificationManager);
    } catch (Exception ex) {
        ex.printStackTrace();
    }

    return zenMode;
}

