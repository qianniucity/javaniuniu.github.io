          <span class="c1">// è·å¾—ServiceManagerçš„getServiceæ–¹æ³•</span>
          <span class="nc">Method</span> <span class="n">getService</span> <span class="o">=</span> <span class="nc">ServiceManager</span><span class="o">.</span><span class="na">getMethod</span><span class="o">(</span><span class="s">"getService"</span><span class="o">,</span> <span class="n">java</span><span class="o">.</span><span class="na">lang</span><span class="o">.</span><span class="na">String</span><span class="o">.</span><span class="na">class</span><span class="o">);</span>
